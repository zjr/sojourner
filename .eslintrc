{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "sjLog": true,
    "sjHttpLog": true
  },
  "rules": {
    "comma-dangle": 0,
    "quotes": [1, "single"],
    "no-undef": 1,
    "strict": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "no-unused-vars": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0,
    "camelcase": 1,
    "global-require": 0,
    "no-param-reassign": [1, { "props": false }]
  }
}

